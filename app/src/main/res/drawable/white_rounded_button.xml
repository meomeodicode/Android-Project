<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
        <item
            android:state_pressed="true">
            <shape>
                <solid
                    android:color="@color/teal_200"/>
                <stroke
                    android:color="@color/white"
                    android:width="1dp"/>
                <corners
                    android:radius="50dp"/>
            </shape>
        </item>
        <item>
            <shape>
                <solid
                    android:color="@color/blue"/>
                <stroke
                    android:color="@color/blue"
                    android:width="1dp"/>
                <corners
                    android:radius="50dp"/>
            </shape>
        </item>
</selector>